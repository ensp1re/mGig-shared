{
  "version": "0.5.7",
  "license": "MIT",
  "main": "index.js",
  "description": "Helpers library for Mgig app",
"scripts": {
  "build": "cross-env NODE_ENV=production npm run build:cjs && cross-env NODE_ENV=production npm run build:esm && tsc --outDir build/src && node scripts/build-package.js",
  "build:cjs": "cross-env NODE_ENV=production BABEL_ENV=cjs babel src --presets=./scripts/babel-preset.js --extensions .ts,.tsx --ignore src/**/*.specs.tsx --out-dir build/cjs --source-maps",
  "build:esm": "cross-env NODE_ENV=production BABEL_ENV=esm babel src --presets=./scripts/babel-preset.js --extensions .ts,.tsx --ignore src/**/*.specs.tsx --out-dir build/esm --source-maps"
},
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "keywords": [],
  "name": "@ensp1re/mgig-shared",
  "author": "ensp1re",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ensp1re/mgig-shared.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "files": [
    "build",
    "src"
  ],
  "exports": {
    ".": {
      "import": "./esm/index.js",
      "require": "./cjs/index.js"
    },
    "./icons": {
      "import": "./esm/icons/index.js",
      "require": "./cjs/icons/index.js"
    }
  },
  "types": "./src/index.d.ts",
  "dependencies": {
    "@elastic/elasticsearch": "^8.13.1",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/winston": "^2.4.4",
    "cloudinary": "^2.2.0",
    "express": "^4.19.2",
    "http-status-codes": "^2.3.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.3.5",
    "winston-elasticsearch": "^0.19.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.5",
    "@babel/core": "^7.24.5",
    "@babel/preset-env": "^7.24.5",
    "@babel/preset-typescript": "^7.24.1",
    "@types/express": "^4.17.21",
    "cross-env": "^7.0.3"
  }
}
